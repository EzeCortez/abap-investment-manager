projection;
strict ( 2 );
use draft;

define behavior for ZC_HEADER_INVE alias Header
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Activate;
  use action Discard;
  use action Resume;
  use action Prepare;

  use association _Schedule { create; with draft; }
}

define behavior for ZC_BONO_SCHEDULE alias Schedule
{
  use update;
  use delete;

  use association _Header { with draft; }
}